{
  "name": "h5webstorage",
  "version": "0.6.4",
  "description": "H5webstorage, an Angular library",
  "scripts": {
    "compile": "tsc",
    "pretest": "npm run compile",
    "test": "node ./tasks/test",
    "test:c": "concurrently \"tsc --watch --sourceMap\" \"karma start test/karma.conf.js --no-single-run\"",
    "build": "node ./tasks/build",
    "start": "webpack-dev-server --open --config ./webpack/webpack.dev.js",
    "lite": "single-page-server -p=8080 -b=./ -f=./example/index.html",
    "docs": "single-page-server -p=8080 -b=./ -f=./dist/docs/index.html",
    "documentation": "typedoc --out ../docs --module commonjs --target es5 --exclude ./src/*.spec.ts --excludeExternals --ignoreCompilerErrors --mode file --excludePrivate --disableOutputCheck ./src/index.ts ./src/basestorage.ts ./src/localstorage.ts ./src/sessionstorage.ts ./src/storageproperty.ts",
    "g": "node ./node_modules/angular-librarian",
    "lint": "tslint ./src/**/*.ts",
    "postbuild": "rimraf build",
    "posttagVersion": "npm run build && npm publish dist",
    "prebuild": "rimraf dist out-tsc",
    "tagVersion": "np --no-publish"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "core-js": "^2.4.1",
    "reflect-metadata": "0.1.2",
    "rxjs": "^5.0.1",
    "zone.js": "0.8.12"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^4.0.0",
    "@types/jasmine": "2.5.38",
    "@types/node": "^6.0.42",
    "angular-librarian": "1.0.0-beta.13",
    "angular2-template-loader": "0.6.0",
    "awesome-typescript-loader": "^3.0.0",
    "codeclimate-test-reporter": "^0.3.1",
    "codelyzer": "~2.0.0-beta.1",
    "concurrently": "^2.0.0",
    "css-loader": "^0.26.1",
    "css-to-string-loader": "^0.1.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.8.5",
    "fs-extra": "^2.1.2",
    "guid": "0.0.12",
    "html-webpack-plugin": "^2.19.0",
    "istanbul-instrumenter-loader": "^1.2.0",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^0.5.5",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-ie-launcher": "^0.2.0",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sauce-launcher": "^0.3.1",
    "karma-source-map-support": "^1.2.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-systemjs": "^0.13.0",
    "karma-webpack": "^2.0.0",
    "node-sass": "^4.1.1",
    "np": "^2.12.0",
    "phantomjs-prebuilt": "^2.1.7",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.3",
    "rollup": "0.43.0",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "3.0.0",
    "rollup-plugin-sourcemaps": "0.4.2",
    "rollup-plugin-uglify": "2.0.1",
    "sass-loader": "^4.0.1",
    "script-loader": "^0.7.0",
    "semver": "5.3.0",
    "single-page-server": "^0.1.0",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "tslint": "^4.0.2",
    "tslint-loader": "^3.3.0",
    "typedoc": "^0.5.10",
    "typescript": "^2.4.2",
    "typings": "^0.8.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^2.2.0",
    "webpack-merge": "^0.14.0",
    "webpack-node-externals": "^1.5.4"
  },
  "peerDependencies": {},
  "main": "./bundles/h5webstorage.umd.js",
  "module": "./h5webstorage.es5.js",
  "es2015": "./h5webstorage.js",
  "typings": "./h5webstorage.d.ts",
  "keywords": [],
  "repository": {
    "url": "https://github.com/SirDarquan/h5webstorage"
  }
