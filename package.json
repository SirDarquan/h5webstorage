{
  "name": "h5webstorage",
  "version": "0.6.2",
  "description": "HTML5 WebStorage API for angular2",
  "scripts": {
    "compile": "tsc",
    "pretest": "npm run compile",
    "test": "karma start test/karma.conf.js",
    "test:c": "concurrently \"tsc --watch --sourceMap\" \"karma start test/karma.conf.js --no-single-run\"",
    "build": "tsc",
    "start": "concurrently \"tsc --watch\" \"npm run lite\"",
    "lite": "single-page-server -p=8080 -b=./ -f=./example/index.html",
    "docs": "single-page-server -p=8080 -b=./ -f=./dist/docs/index.html",
    "documentation": "typedoc --out ../docs --module commonjs --target es5 --exclude ./src/*.spec.ts --excludeExternals --ignoreCompilerErrors --mode file --excludePrivate --disableOutputCheck ./src/index.ts ./src/basestorage.ts ./src/localstorage.ts ./src/sessionstorage.ts ./src/storageproperty.ts"
  },
  "author": "SirDarquan <https://github.com/SirDarquan>",
  "license": "ISC",
  "dependencies": {
    "@angular/common": "^2.0.1",
    "@angular/compiler": "^2.0.1",
    "@angular/core": "^2.0.1",
    "@angular/forms": "^2.0.1",
    "@angular/platform-browser": "^2.0.1",
    "@angular/platform-browser-dynamic": "^2.0.1",
    "es6-shim": "^0.35.1",
    "reflect-metadata": "0.1.2",
    "rxjs": "^5.0.0-beta.12",
    "systemjs": "^0.19.26",
    "zone.js": "^0.6.25"
  },
  "devDependencies": {
    "@types/es6-shim": "^0.31.32",
    "@types/jasmine": "^2.5.38",
    "codeclimate-test-reporter": "^0.3.1",
    "concurrently": "^2.0.0",
    "guid": "0.0.12",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^0.2.3",
    "karma-coverage": "^0.5.5",
    "karma-ie-launcher": "^0.2.0",
    "karma-jasmine": "^0.3.8",
    "karma-sauce-launcher": "^0.3.1",
    "karma-source-map-support": "^1.2.0",
    "karma-systemjs": "^0.13.0",
    "single-page-server": "^0.1.0",
    "typedoc": "^0.5.1",
    "typescript": "^2.0.0"
  },
  "peerDependencies": {}
}
